{
	"name": "ai-shoping-list",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"dev": "react-router dev",
		"start": "react-router-serve ./build/server/index.js",
		"lint": "biome lint",
		"lint:write": "biome lint --write",
		"format": "biome format",
		"format:write": "biome format --write",
		"check": "biome check",
		"check:write": "biome check --write",
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"typecheck": "react-router typegen && tsc",
		"prisma:migrate:dev": "dotenv -e .env.local -- pnpx prisma migrate dev",
		"prisma:migrate:reset": "dotenv -e .env.local -- pnpx prisma migrate reset",
		"prisma:migrate:deploy": "dotenv -e .env.local -- pnpx prisma migrate deploy",
		"prisma:format": "dotenv -e .env.local -- pnpx prisma format",
		"prisma:generate": "dotenv -e .env.local -- pnpx prisma generate",
		"prisma:studio": "dotenv -e .env.local -- pnpx prisma studio",
		"prisma:db:pull": "dotenv -e .env.local -- pnpx prisma db pull",
		"prisma:db:push": "dotenv -e .env.local -- pnpx prisma db push",
		"prisma:db:seed": "dotenv -e .env.local -- pnpx prisma db seed",
		"prisma:db:validate": "dotenv -e .env.local -- pnpx tsx prisma/validate-setup.ts",
		"ui": "bunx shadcn@latest",
		"prepare": "husky",
		"postinstall": "prisma generate",
		"commit": "cz"
	},
	"dependencies": {
		"@neondatabase/serverless": "^1.0.0",
		"@prisma/adapter-neon": "^6.8.2",
		"@prisma/client": "^6.8.2",
		"@react-router/node": "^7.6.1",
		"@react-router/serve": "^7.6.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"isbot": "^5.1.28",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router": "^7.6.1",
		"tailwind-merge": "^3.3.0",
		"vite-plugin-babel": "^1.3.1",
		"ws": "^8.18.2",
		"zod": "^3.25.28"
	},
	"devDependencies": {
		"@babel/preset-typescript": "^7.27.1",
		"@biomejs/biome": "1.9.4",
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@commitlint/types": "^19.8.1",
		"@react-router/dev": "^7.6.1",
		"@tailwindcss/vite": "^4.1.7",
		"@types/node": "^22.15.21",
		"@types/react": "^19.1.5",
		"@types/react-dom": "^19.1.5",
		"@types/ws": "^8.18.1",
		"babel-plugin-react-compiler": "^19.1.0-rc.2",
		"commitizen": "^4.3.1",
		"cz-conventional-changelog": "^3.3.0",
		"dotenv-cli": "^8.0.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.0.0",
		"prisma": "^6.8.2",
		"tailwindcss": "^4.1.7",
		"tsx": "^4.19.4",
		"turbo": "^2.5.3",
		"tw-animate-css": "^1.3.0",
		"typescript": "^5.8.3",
		"vite": "^6.3.5",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.1.4"
	},
	"engines": {
		"node": ">=22.15.0",
		"npm": "please-use-bun",
		"yarn": "please-use-bun",
		"pnpm": "please-use-bun",
		"bun": ">=1.1.34"
	},
	"packageManager": "bun@1.1.34"
}
